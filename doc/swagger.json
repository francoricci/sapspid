              "type": "integer",
              "minimum": 0,
              "maximum": 0
            },
            "httpcode": {
              "type": "integer",
              "minimum": 200,
              "maximum": 200
            },
            "message": {
              "type": "string",
              "pattern": "^OK$",
              "minLength": 1
            }
          },
          "additionalProperties": false
        },
        "id": {
